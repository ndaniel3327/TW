<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TW.UI.Pages.SpotifyPlaylistsPage"
             Title="SpotifyPlaylistsPage"
             xmlns:viewmodel="clr-namespace:TW.UI.Pages"
             x:DataType="viewmodel:SpotifyPlaylistsPage">
    <StackLayout>
        <ListView x:Name="listView" ItemsSource="{Binding Playlists}">
            <ListView.ItemTemplate>
                <DataTemplate x:DataType="viewmodel:PlaylistModel">
                    <TextCell Text="{Binding Name}"></TextCell>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </StackLayout>

</ContentPage>