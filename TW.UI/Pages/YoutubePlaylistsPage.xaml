<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TW.UI.Pages.YoutubePlaylistsPage"
             Title="YoutubePlaylistsPage"
             xmlns:viewmodel="clr-namespace:TW.UI.Pages"
             x:DataType="viewmodel:YoutubePlaylistsPage"
             xmlns:model="clr-namespace:TW.UI.Services.Youtube">
    <StackLayout>
        <ListView x:Name="listView"
                  VerticalOptions="FillAndExpand"
                  IsGroupingEnabled="True"
                  ItemsSource="{Binding YoutubePlaylists}">
            <ListView.ItemTemplate >
                <DataTemplate  x:DataType="model:YoutubePlaylistModel">
                    <TextCell Text="{Binding Name}" Height="100"></TextCell>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </StackLayout>
</ContentPage>