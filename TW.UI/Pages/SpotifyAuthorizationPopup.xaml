<?xml version="1.0" encoding="utf-8" ?>
<mct:Popup xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
           xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
           xmlns:mct="clr-namespace:CommunityToolkit.Maui.Views;assembly=CommunityToolkit.Maui"
           xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
           x:Class="TW.UI.Pages.SpotifyAuthorizationPopup"
           xmlns:viewmodel="clr-namespace:TW.UI.ViewModels"
           x:DataType="viewmodel:SpotifyAuthorizationPopupViewModel"
           Size="{Binding ScreenSize}" Closed="Popup_Closed">
     <WebView x:Name="webView" Source="{Binding LoginUri}"  PropertyChanged="WebViewSourceChanged" Navigating="webView_Navigating" BindingContextChanged="webView_BindingContextChanged" Navigated="webView_Navigated"></WebView>
</mct:Popup>